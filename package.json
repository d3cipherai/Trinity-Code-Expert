{
  "name": "trinity-awaken",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "mcp:start": "tsx mcp-orchestra/server.ts",
    "mcp:dev": "tsx watch mcp-orchestra/server.ts",
    "trinity:full": "concurrently \"npm run dev\" \"npm run mcp:dev\"",
    "trinity:build": "npm run build && tsx mcp-orchestra/server.ts",
    "setup:deciphergit": "tsx scripts/setup-deciphergit.ts",
    "memory:sync": "tsx scripts/sync-memory.ts",
    "create:shortcuts": "powershell -ExecutionPolicy Bypass -File scripts/create-shortcuts.ps1",
    "startup": "scripts/start-trinity.bat"
  },
  "dependencies": {
    "fs-extra": "^11.3.1",
    "next": "15.4.5",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.0",
    "eslint": "^8",
    "eslint-config-next": "15.4.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
