{
  "name": "trinity-awaken",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "mcp:start": "tsx mcp-orchestra/server.ts",
    "mcp:dev": "tsx watch mcp-orchestra/server.ts",
    "trinity:full": "concurrently \"npm run dev\" \"npm run mcp:dev\"",
    "trinity:chat": "concurrently \"npm run dev\" \"npm run mcp:dev\" \"npm run mcp:bridge\"",
    "trinity:build": "npm run build && tsx mcp-orchestra/server.ts",
    "trinity:status": "powershell -ExecutionPolicy Bypass -File scripts/trinity-status-check.ps1",
    "status": "npm run trinity:status",
    "teams:install": "cd teams-bot && npm install",
    "teams:dev": "cd teams-bot && npm run dev",
    "teams:build": "cd teams-bot && npm run build",
    "teams:start": "cd teams-bot && npm start",
    "superwoman:start": "tsx teams-integration/trinity-superwoman.ts",
    "superwoman:dev": "tsx watch teams-integration/trinity-superwoman.ts",
    "superwoman:test": "tsx teams-integration/trinity-superwoman-test.ts",
    "mcp:bridge": "tsx teams-integration/mcp-teams-bridge.ts",
    "mcp:bridge:dev": "tsx watch teams-integration/mcp-teams-bridge.ts",
    "trinity:superwoman": "npm run superwoman:start",
    "setup:deciphergit": "tsx scripts/setup-deciphergit.ts",
    "memory:sync": "tsx scripts/sync-memory.ts",
    "create:shortcuts": "powershell -ExecutionPolicy Bypass -File scripts/create-shortcuts.ps1",
    "startup": "scripts/start-trinity.bat"
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^3.0.4",
    "@azure/msal-node": "^2.6.0",
    "botbuilder": "^4.20.0",
    "express": "^4.18.2",
    "fs-extra": "^11.3.1",
    "next": "15.4.5",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.0",
    "eslint": "^8",
    "eslint-config-next": "15.4.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
